{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
{
  "routes": [
    { "src": "/playlist-downloader", "dest": "/playlist-downloader.html" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}

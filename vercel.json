{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/playlist-downloader",
      "dest": "/playlist-downloader.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}

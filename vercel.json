{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
   {
  "rewrites": [
    {
      "source": "/musicxz",
      "destination": "/web/music-xz.html"
    }
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
